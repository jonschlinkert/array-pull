{
  "name": "array-pull",
  "description": "Remove the specified elements from the given array using strict equality for comparison.",
  "version": "0.2.0",
  "homepage": "https://github.com/jonschlinkert/array-pull",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "repository": "jonschlinkert/array-pull",
  "bugs": {
    "url": "https://github.com/jonschlinkert/array-pull/issues"
  },
  "license": "MIT",
  "files": [
    "index.js"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "test": "mocha"
  },
  "dependencies": {
    "arr-flatten": "^1.0.1",
    "array-slice": "^0.2.3"
  },
  "devDependencies": {
    "ansi-bold": "^0.1.1",
    "benchmarked": "^0.1.4",
    "glob": "^5.0.5",
    "gulp-format-md": "^0.1.7",
    "mocha": "^2.2.4",
    "should": "^6.0.1"
  },
  "keywords": [
    "array",
